{
    "name": "hasura-extra/auth-hook",
    "license": "MIT",
    "description": "Webhook authentication for Hasura.",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Hasura\\AuthHook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hasura\\AuthHook\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-handler": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "nyholm/psr7": "^1.0"
    }
}
